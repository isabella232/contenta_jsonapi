{
  "name": "contentacms/contenta_jsonapi",
  "description": "",
  "type": "drupal-profile",
  "authors": [
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
